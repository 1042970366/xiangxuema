<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>想学吗-个人知识管理工具</title>
  <% if (htmlWebpackPlugin.options.nodeModules) { %>
  <!-- Add `node_modules/` to global paths so `require` works properly in development -->
  <script>
    require('module').globalPaths.push('<%= htmlWebpackPlugin.options.nodeModules.replace(/\\/g, '\\\\') %>')
      </script>
  <% } %>
  <link href="https://at.alicdn.com/t/font_449040_so8jvd25yz.css" rel="stylesheet" type="text/css" />
  <link href="./static/tui/tui-editor.css" rel="stylesheet" type="text/css" />
  <link href="./static/tui/tui-editor-contents.css" rel="stylesheet" type="text/css" />
  <link href="./static/tui/codemirror.css" rel="stylesheet" type="text/css" />
  <link href="./static/tui/github.min.css" rel="stylesheet" type="text/css" />
  <script src="./static/tui/tui-editor-Editor-full.js"></script>
  <script src="./static/neditor/neditor.config.js"></script>
  <script src="./static/neditor/neditor.all.js"></script>
  <script src="./static/neditor/neditor.service.js"></script>
  <script src="./static/neditor/i18n/zh-cn/zh-cn.js"></script>
  <script src="./static/neditor/kityformula-plugin/addKityFormulaDialog.js"></script>
  <script src="./static/neditor/kityformula-plugin/getKfContent.js"></script>
  <script src="./static/neditor/kityformula-plugin/defaultFilterFix.js"></script>
  <style>
    #edui162_body {
      background: url('./static/neditor/kityformula-plugin/kf-icon.png') no-repeat center center !important;
    }

    .edui-notadd .edui-editor {
      border: none !important;
      position: inherit !important;
      height: 100%;
      display: flex;
      flex-flow: column;
    }

    .edui-editor-bottomContainer .edui-notadd {
      display: none !important;
    }

    .edui-notadd .edui-editor-toolbarbox {
      position: inherit !important;
      zoom: normal !important;
      border-top-left-radius: 0px !important;
      border-top-right-radius: 0px !important;
    }

    .edui-notadd .edui-editor-iframeholder {
      position: inherit !important;
      flex: 1;
      width: 100% !important;
    }

    .CodeMirror-wrap {
      position: initial !important;
    }

    .cm-s-default {
      position: initial !important;
    }

    .edui-notadd .edui-editor-bottomContainer {
      display: none !important;
    }
  </style>
</head>

<body>
  <div id="app"></div>
  <!-- Set `__static` path to static files in production -->
  <% if (!process.browser) { %>
  <script>
    if (process.env.NODE_ENV !== 'development') window.__static = require('path').join(__dirname, '/static').replace(
      /\\/g, '\\\\')
  </script>
  <% } %>
  <!-- webpack builds are automatically injected -->
</body>

</html>